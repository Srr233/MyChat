{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error", {
           "endOfLine":"auto",
           "useTabs": true
        }],
        "no-tabs":"off",
        "indent":"off",
        "linebreak-style": [
            "error",
            "windows"
        ]
    }
}